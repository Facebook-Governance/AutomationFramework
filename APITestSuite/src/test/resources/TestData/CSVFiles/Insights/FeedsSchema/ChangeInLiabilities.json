            "accountName": {
              "type": "string"
            },
            "accountType": {
              "type": "string"
            },
            "isAsset": {
              "type": "boolean"
            },
            "container": {
              "type": "string"
            },
            "link": {
              "type": "object",
              "properties": {
                "entityName": {
                  "type": "string"
