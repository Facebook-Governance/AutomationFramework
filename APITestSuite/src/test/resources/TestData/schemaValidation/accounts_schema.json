            "examples": [
              "16441"
            ]
          }, 
          "providerAccountId": {
            "$id": "/properties/account/items/properties/providerAccountId", 
            "type": "integer", 
            "title": "The Provideraccountid Schema.", 
            "description": "An explanation about the purpose of this instance.", 
            "default": 0, 
            "examples": [
              11940852
